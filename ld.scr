OUTPUT_FORMAT("h8300-linux-elf")
OUTPUT_ARCH(h8300h)
ENTRY("_start")

SECTIONS
{
    . = 0x0;

    .vectors : {
        build/vector.c.o(.data)
    }

    .text : {
        *(.text)
    }

    .rodata : {
        *(.strings)
        *(.rodata)
        *(.rodata.*)
    }

    .data : {
        *(.data)
    }

    .bss : {
        *(.bss)
        *(COMMON)
    }
}
